{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {},
  "overrides": [
    {
      "files": ["**/*.{js,jsx,ts,tsx}"],
      "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "extends": ["plugin:prettier/recommended"],
      "rules": {}
    }
  ]
}
